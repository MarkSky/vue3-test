        "pinia": "^2.3.1",
        "vue": "^3.5.13",
        "vue-i18n": "^11.1.0",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.19.0",
        "@eslint/markdown": "^6.2.2",
        "@html-eslint/eslint-plugin": "^0.34.0",

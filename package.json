{
    "name": "vue3-test",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:e2e": "playwright test",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
        "lint:eslint": "eslint . --fix",
        "lint": "run-s lint:*"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild",
            "protobufjs",
            "sharp",
            "vue-demi"
        ],
        "supportedArchitectures": {
            "os": [
                "window",
                "win32",
                "darwin",
                "current"
            ],
            "cpu": [
                "x64",
                "arm64",
                "arm",
                "current"
            ]
        }
    },
    "dependencies": {
        "@master/colors": "^2.1.0",
        "@master/css": "2.0.0-rc.48",
        "@master/css.vue": "2.0.0-rc.48",

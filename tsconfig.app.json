{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "router.d.ts",
        "typed-router.d.ts",
        "master.css.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "exclude": [
        "src/**/__tests__/*"
    ],
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "types": [
            "unplugin-vue-router/client"
        ],
        "moduleResolution": "Bundler"
    }
}

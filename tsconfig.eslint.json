{
    "extends": "./tsconfig.app.json",
    "include": [
        "eslint.config.ts",
        "*.d.ts",
        "shared/models/**/*",
        "src/models/**/*",
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.vue"
    ],
    "exclude": [
